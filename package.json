{
    "name": "nano-git",
    "version": "0.1.0",
    "description": "A minimal Git implementation for educational purposes",
    "keywords": [
        "git"
    ],
    "private": true,
    "homepage": "https://github.com/timokoessler/nano-git",
    "bugs": "https://github.com/timokoessler/nano-git/issues",
    "repository": "timokoessler/nano-git",
    "license": "MIT",
    "author": "Timo KÃ¶ssler",
    "type": "module",
    "exports": "./lib/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "ngit": "./bin/run.js"
    },
    "files": [
        "/bin",
        "/dist",
        "/oclif.manifest.json"
    ],
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "prepack": "npm run build && oclif manifest && oclif readme",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepare": "npm run build",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "posttest": "npm run lint",
        "version": "oclif readme && git add README.md"
    },
    "oclif": {
        "bin": "ngit",
        "commands": "./dist/commands",
        "dirname": "ngit",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    },
    "dependencies": {
        "@oclif/core": "^3",
        "@oclif/plugin-help": "^6"
    },
    "devDependencies": {
        "@oclif/prettier-config": "^0.2.1",
        "@oclif/test": "^3",
        "@types/chai": "^4",
        "@types/mocha": "^10",
        "@types/node": "^20.12.2",
        "chai": "^4.4.1",
        "eslint": "^8",
        "eslint-config-oclif": "^5",
        "eslint-config-oclif-typescript": "^3",
        "eslint-config-prettier": "^9.1.0",
        "mocha": "^10",
        "oclif": "^4.7.3",
        "prettier": "^3.2.5",
        "shx": "^0.3.4",
        "ts-node": "^10.9.2",
        "typescript": "5.3"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
