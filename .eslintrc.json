{
    "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "indent": ["off", "tab"],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"]
    }
}
